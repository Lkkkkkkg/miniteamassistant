page {
  background-color: #f5f6f7;
}

.page-header {
  padding-bottom: 32rpx;
  background-color: #fff;
}

.nickname {
  text-align: center;
  font-size: 36rpx;
  color: #333;
}

.login-text {
  height: 48rpx;
  text-align: center;
  font-size: 0;
  color: #666;
  transition: opacity .45s cubic-bezier(.23,1,.32,1);
}

.login-text-hover {
  opacity: .6;
}

.button-label {
  font-size: 36rpx;
}

