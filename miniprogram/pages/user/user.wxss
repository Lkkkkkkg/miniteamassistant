.avatar-wrap {
  margin-top: 26rpx;
  display: flex;
  justify-content: center;
}

.avatar-border {
  display: inline-block;
  border-radius: 50%;
  border: 1rpx solid #999;
}

.avatar {
  margin: 2rpx;
  display: block;
  width: 146rpx;
  height: 146rpx;
  border-radius: 50%;
}

.extra {
  display: flex;
  margin: 32rpx 0 0;
  align-items: center;
}

.extra-item {
  flex: 1;
  text-align: center;
}

.extra-num {
  font-size: 40rpx;
  color: #555;
}

.extra-label {
  font-size: 27rpx;
  color: #555;
}

.panel {
  margin-top: 20rpx;
  background-color: #fff;
}

.panel-item-wrap {
  padding-left: 50rpx;
  transition: background-color 0.45s cubic-bezier(0.23, 1, 0.32, 1);
}

.panel-item-wrap-hover {
  background-color: #e5e6e7;
}

.panel-item {
  height: 106rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #eaeaea;
}

.panel-item-content {
  flex: 1;
}

.panel-item-wrap:last-child .panel-item {
  border: none;
}

.panel-icon {
  margin-right: 16rpx;
}

.icon {
  display: block;
  width: 40rpx;
  height: 40rpx;
  background-size: cover;
}

