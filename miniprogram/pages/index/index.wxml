<view class="page">
  <view class="cards">
    <view class="card" wx:for="{{teamList}}" wx:key="{{item._id}}">
      <image class="card-img" src="{{item.gameIcon}}"></image>
      <view class="card-content">
        <view>预计开黑时间：{{item.startTime}}</view>
        <view>{{item.participant.length}}/{{item.maxNum}}</view>
        <view>等待中</view>
      </view>
      <!-- <button class="button-join" open-type="getUserInfo" bindgetuserinfo="bindgetuserinfo">加</button> -->
    </view>
  </view>
  <navigator class="button-add" url="./pages/edit/edit">发</navigator>
</view>