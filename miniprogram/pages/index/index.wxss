.empty-text {
  font-size: 28rpx;
  font-weight: bold;
  color: #777;
}

.button-add {
  position: fixed;
  right: 30rpx;
  bottom: 30rpx;
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background-color: #fff;
  border-radius: 50%;
  box-shadow: 0 0 5rpx #bbb;
  font-size: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  transition: background-color 0.45s cubic-bezier(0.23, 1, 0.32, 1);
}

.button-add-icon {
  width: 70rpx;
  height: 70rpx;
}

.button-add-hover {
  background-color: #f5f6f7;
}

.button-add-loading {
  background-color: #f5f6f7;
}

.mask {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 1;
  background-color: rgba(0, 0, 0, 0.4);
  visibility: hidden;
  opacity: 0;
  transition: opacity 0.45s cubic-bezier(0.23, 1, 0.32, 1), visibility 0s 0.45s;
}

.mask-show {
  visibility: visible;
  opacity: 1;
  transition: opacity 0.45s cubic-bezier(0.23, 1, 0.32, 1);
}

.picker-wrap {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 2;
  transform: translate3d(0, 321px, 0);
  transition: transform 0.45s cubic-bezier(0.23, 1, 0.32, 1);
  border-top-left-radius: 12px;
  border-top-right-radius: 12px;
  background-color: #fff;
  padding: 0 24px;
  padding: 0 calc(24px + constant(safe-area-inset-right)) constant(safe-area-inset-bottom) calc(24px + constant(safe-area-inset-left));
  padding: 0 calc(24px + env(safe-area-inset-right)) env(safe-area-inset-bottom) calc(24px + env(safe-area-inset-left));
}

.picker-wrap.show {
  transform: translate3d(0, 0, 0);
}

.picker-header {
  display: flex;
  justify-content: space-between;
  height: 100rpx;
  color: #1ca2ff;
  line-height: 100rpx;
}

.picker-title {
  color: #333;
}

.button-submit, .button-confirm, .button-cancel {
  transition: opacity 0.45s cubic-bezier(0.23, 1, 0.32, 1);
}

.button-submit-hover, .picker-button-hover {
  opacity: 0.5;
}
