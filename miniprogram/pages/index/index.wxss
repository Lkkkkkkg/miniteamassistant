  font-size: 0;
  background-color: #1ca2ff;
  color: #fff;
  border-radius: 50rpx;
  transition: opacity 0.45s cubic-bezier(0.23, 1, 0.32, 1);
}

.card-button-hover, .card-button-loading {
  opacity: 0.6;
}

.button-label {
  font-size: 30rpx;
  text-align: center;
}

.card-disabled-icon {
  position: absolute;
