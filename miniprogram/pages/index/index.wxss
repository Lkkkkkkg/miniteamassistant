  margin-top: 8rpx;
  width: 230rpx;
  height: 230rpx;
  border-radius: 10rpx;
  border: 1rpx solid #ddd;
  overflow: hidden;
}

.card-logo {
  width: 100%;
  height: 100%;
}

.card-time, .card-num {
  display: flex;
  align-items: center;
  margin-top: 6rpx;
  font-size: 29rpx;
  color: #666;
}

.card-icon {
  width: 40rpx;
  height: 40rpx;
}

.card-participant {
  display: flex;
  color: #666;
  align-items: center;
  height: 70rpx;
  font-size: 0;
}

.card-creator {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  padding: 20rpx 40rpx;
  display: flex;
  align-items: center;
  font-size: 30rpx;
}

.card-creator-avatar {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

