.page-header {
  position: sticky;
  top: 0;
  left: 0;
  z-index: 1;
  padding: 0 30rpx;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  border-bottom: 2rpx solid #ddd;
}

.tabs {
  position: relative;
  display: inline-flex;
}

.tab {
  width: 60px;
  text-align: center;
  font-size: 34rpx;
  color: #333;
  transition: color 0.225s cubic-bezier(0.23, 1, 0.32, 1);
}

.tab-0-hover {
  color: #1ca2ff;
}

.tab-0-hover .tab-bar {
  transform: translate3d(0, 0, 0) !important;
}

.tab-1-hover {
  color: #1ca2ff;
}

.tab-1-hover .tab-bar {
  transform: translate3d(120rpx, 0, 0) !important;
}

.tab-2-hover {
  color: #1ca2ff;
}

.tab-2-hover .tab-bar {
  transform: translate3d(240rpx, 0, 0) !important;
}

.tab-3-hover {
  color: #1ca2ff;
}

.tab-3-hover .tab-bar {
  transform: translate3d(360rpx, 0, 0) !important;
}

.tab-bar {
  position: absolute;
  left: 14px;
  bottom: -2rpx;
  height: 4rpx;
  width: 34px;
  background-color: #1ca2ff;
  z-index: 1;
  transition: transform 0.225s cubic-bezier(0.23, 1, 0.32, 1);
}

.tab.active {
  color: #1ca2ff;
}

.cards-info {
  position: sticky;
  z-index: 1;
  top: 102rpx;
  left: 0;
  height: 72rpx;
  line-height: 72rpx;
  padding: 0 30rpx;
  display: flex;
  justify-content: space-between;
  font-size: 29rpx;
  background-color: #fff;
}

.num {
  color: #888;
}

.filter {
  display: flex;
  align-items: center;
  color: #555;
  transition: opacity 0.45s cubic-bezier(0.23, 1, 0.32, 1);
}

.filter-icon {
  margin-left: 6rpx;
  display: inline-block;
  width: 20rpx;
  height: 20rpx;
  background-image: url("data:image/svg+xml,%3Csvg t='1573615137762' class='icon' style='width: 1em; height: 1em;vertical-align: middle;fill: currentColor;overflow: hidden;' viewBox='0 0 1024 1024' version='1.1' xmlns='http://www.w3.org/2000/svg' p-id='33211'%3E%3Cpath d='M919.8 381.1L555.2 779.8a57.377 57.377 0 0 1-43.1 19.5c-16.5 0-32.2-7.1-43.1-19.5L104.4 381.1c-17.8-18.8-23.3-46.2-14.1-70.4s31.4-41.1 57.2-43.4h728.9c25.9 2.2 48.2 19 57.4 43.3 9.3 24.3 3.8 51.6-14 70.5z' fill='%23555555' p-id='33212'%3E%3C/path%3E%3C/svg%3E");
}

.filter-hover {
  opacity: 0.6;
}

.card-wrap {
  padding: 0 30rpx;
  transition: background-color 0.45s cubic-bezier(0.23, 1, 0.32, 1);
}

.card-wrap-hover {
  background-color: #f4f5f6;
}

.card {
  position: relative;
  display: flex;
  justify-content: space-between;
  font-size: 0;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #ddd;
}

.card-left {
  font-size: 30rpx;
  color: #333;
  flex: 1;
  width: 0;
}

.card-title {
  vertical-align: middle;
  font-size: 0;
  font-weight: bold;
}

.card-title-text {
  vertical-align: middle;
  display: inline;
  font-size: 34rpx;
  font-weight: bold;
}

