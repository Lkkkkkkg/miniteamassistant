.card-dialog {
  position: relative;
  font-size: 0;
}

.card-dialog-text {
  font-size: 28rpx;
  margin-top: 4rpx;
  margin-bottom: 4rpx;
  color: #eb981d8e;
  padding: 2rpx 10rpx;
  border-radius: 2rpx;
  background: #eee;
  display: inline-block;
  max-width: 456rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

